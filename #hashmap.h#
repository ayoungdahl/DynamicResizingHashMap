#ifndef AKY_HASH_HASHMAP_H
#define AKY_HASH_HASHMAP_H


#define NUM_BUCKETS 16
#define NUM_NODES_IN_BUCKET 10

namespace akyhash {
  template<typename K, typename V>
  class HashMap {
  public:
  HashMap(hashFunc = std::Hash<K>, keyEQFunc = std::equal_to<K>) : numBuckets(NUM_BUCKETS), numNodesInBucket(16),  {}
 
  private:
    int numBuckets;
    const int numNodesInBucket;
  };
}
#endif
  